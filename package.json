{
  "name": "leaflet-tiny-log-collector",
  "version": "1.1.0-dev",
  "bin": "./dist/tlc-main.js",
  "scripts": {
    "start": "ts-node ./src/tlc-main.ts",
    "diststart": "node ./dist/src/tlc-main.js",
    "livereload": "ts-node-dev --respawn --pretty --transpile-only ./src/tlc-main.ts",
    "test": "jest",
    "build": "tsc -p . && tsc-alias -p tsconfig.json && ts-node .circleci/generate-build-time.ts"
  },
  "dependencies": {
    "axios": "1.6.8",
    "config": "3.3.11",
    "js-yaml": "4.1.0",
    "jsonpath-plus": "8.1.0",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@types/config": "3.3.4",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.11",
    "@types/sinon": "17.0.3",
    "jest": "29.7.0",
    "sinon": "17.0.2",
    "tsconfig-paths": "4.2.0",
    "tsc-alias": "1.8.8",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.5"
  }
}
